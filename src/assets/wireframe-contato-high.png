  <rect x="810" y="650" width="70" height="20" fill="white" stroke="#e0e0e0" stroke-width="1" rx="10"/>
  <text x="845" y="662" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">Instagram</text>
  
  <!-- Map Section -->
  <rect x="0" y="720" width="1200" height="60" fill="#e0e0e0"/>
  <text x="600" y="735" font-family="Arial" font-size="20" font-weight="bold" text-anchor="middle" fill="#333">Nossa Localização</text>
  <circle cx="600" cy="755" r="15" fill="#666"/>
  <text x="600" y="762" font-family="Arial" font-size="12" fill="white" text-anchor="middle">📍</text>
  <text x="600" y="775" font-family="Arial" font-size="12" text-anchor="middle" fill="#666">Rua do Imperador, 350 - Centro, Petrópolis/RJ</text>
</svg>
